<schema>
    <element name="banks">
        <complexType>
            <sequence>
                <element name="account" type="bankAccount" minOccurs="1" maxOccurs="unbounded" />
            </sequence>
        </complexType>
        <complexType name="bankAccount">
            <sequence>
                <element name="name" type="string"/>
                <element name="country" type="string"/>
                <element name="deposit" type="enumeration"/>
                
            </sequence>
        </complexType>
    </element>
</schema>